{
    "editor.formatOnSave": true,
    "eslint.format.enable": true,
    "cSpell.ignoreWords": [
        "adminer",
        "copyhistory",
        "knexfile",
        "postgresql"
    ],
    "cSpell.words": [
        "Concatenator",
        "Postgis",
        "SRID",
        "dwithin",
        "jsonb",
        "livr",
        "minio"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript"
    ]
}